import java.util.Scanner;

public class FunctionsByShradda {
// we will write return type int because this time it will return sum of two numbers
public static int calculateSum(int a,int b){
    int sum=a+b;
    return sum;
}

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
         int c=calculateSum(a,b);
         System.out.println(c);
        // int sum = calculateSum(a, b);
        // System.out.println("sum of two number is :"+ calculateSum(a, b));
        // System.out.println(calculateSum(a, b));
        // calculateSum(a,b);
    }
}
